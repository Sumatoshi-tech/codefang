    type: "List",
    roles: "Literal"
)

array_type <- (array_type) => uast(
    type: "TypeAnnotation",
    roles: "Type"
)

slice_type <- (slice_type) => uast(
    type: "TypeAnnotation",
    roles: "Type"
)

pointer_type <- (pointer_type) => uast(
    type: "TypeAnnotation",
    roles: "Type"
)

error_union_type <- (error_union_type) => uast(
    type: "TypeAnnotation",
    roles: "Type"
)

payload <- (payload) => uast(
    type: "Variable",
    roles: "Variable"
)

labeled_statement <- (labeled_statement) => uast(
    type: "Synthetic"
)

expression_statement <- (expression_statement) => uast(
    type: "Synthetic"
)

comment <- (comment) => uast(
    token: "self",
    type: "Comment",
    roles: "Comment"
)

doc_comment <- (doc_comment) => uast(
    token: "self",
    type: "Comment",
    roles: "Comment"
)

escape_sequence <- (escape_sequence) => uast(
    type: "Synthetic"
)

comptime_block <- (comptime_block) => uast(
    type: "Block",
    roles: "Body"
)

asm_expression <- (asm_expression) => uast(
    type: "Call",
    roles: "Call"
)

suspend_expression <- (suspend_expression) => uast(
