[language "gitignore", extensions: ".gitignore"]

document <- (document) => uast(
    type: "File"
)

comment <- (comment) => uast(
    token: "self",
    type: "Comment",
    roles: "Comment"
)

pattern <- (pattern) => uast(
    token: "self",
    type: "Literal",
    roles: "Literal"
)

pattern_char <- (pattern_char) => uast(
    token: "self",
    type: "Synthetic"
)

pattern_char_escaped <- (pattern_char_escaped) => uast(
    token: "self",
    type: "Synthetic"
)

negation <- (negation) => uast(
    token: "self",
    type: "Synthetic"
)

